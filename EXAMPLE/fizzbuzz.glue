import PATH

#############################
# fizzbuzz.glue (for Mac)   #
#############################

# make a file "nums"
file nums = seq 1 100

#gsed can be installed after "brew install coreutils"
file buzz = gsed '5~5s/.*/Buzz/' nums

#output
/usr/local/bin/gsed '3~3s/[0-9]*/Fizz/' buzz

# Files (nums and buzz) are automatically erased.
