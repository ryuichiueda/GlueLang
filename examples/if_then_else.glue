# for mac
# in the case of linux, true and false commands exist in /bin/
# so it should be replaced.

{
	/bin/echo 'OK'
} !> {
	/bin/echo 'a'
	/bin/echo 'no'
} !> {
	/bin/echo 'ow'
}

{
	/usr/bin/false
	/bin/echo 'NG'
} !> {
	/bin/echo 'a'
	/bin/echo 'OK'
}

{
	/usr/bin/false
	/bin/echo 'NG'
} !> {
	/bin/echo 'OK'
}
